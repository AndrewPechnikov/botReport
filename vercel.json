{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook",
      "methods": ["POST"]
    }
  ],
  "functions": {
    "api/webhook.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "env": {
    "PYTHONPATH": ".",
    "TZ": "Europe/Kiev"
  }
} 